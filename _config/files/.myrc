}


function mtssh {
    ssh-keygen -f safe
    chmod 600 safe
    cat safe.pub
    echo "echo $(cat safe.pub) > authorized_keys" | clip
}


function hex2bin {
  if [[ -z $1 ]]; then
    echo "Usage: hex2bin <file.bin>"
    return
  fi
  echo "$(cat "$1")" | xxd -r -p
}

function bin2hex {
  if [[ -z $1 ]]; then
    echo "Usage: bin2hex <binary>"
    return
  fi
  
  hexdump -v -e '1/1 "%02x"' "$1"
}

